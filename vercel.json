{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install && chmod -R 755 node_modules/.bin",
  "framework": "create-react-app",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/logo192.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
